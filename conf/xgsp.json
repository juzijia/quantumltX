{
  "name": "西瓜视频",
  "type": "rewrite",
  "note": "西瓜视频器重写订阅",
  "author": "Orange",
  "resource": "https://raw.githubusercontent.com/juzijia/quantumltX/main/conf/xgsp.json",
  "mitmhost": [
    "*.ixigua.com"
  ],
  "rewrite": [
    {
      "match": "/luckycat/xigua/v1/task/page?",
      "target": "xgsp_sync.js",
      "note": "任务主页",
      "enable": true,
      "stage": "res"
    },
    {
      "match": "/luckycat/xigua/v1/task/done/coin_chest?",
      "target": "xgsp_sync.js",
      "note": "开宝箱",
      "enable": true,
      "stage": "res"
    },
    {
      "match": "/luckycat/xigua/v1/task/done/coin_chest_ad?",
      "target": "xgsp_sync.js",
      "note": "宝箱广告",
      "enable": true,
      "stage": "res"
    },
    {
      "match": "/luckycat/xigua/v1/task/query/ad_watch_daily_again?",
      "target": "xgsp_sync.js",
      "note": "再看广告get",
      "enable": true,
      "stage": "res"
    },
    {
      "match": "/luckycat/xigua/v1/task/done/ad_watch_daily_again?",
      "target": "xgsp_sync.js",
      "note": "再看广告post",
      "enable": true,
      "stage": "res"
    },
    {
      "match": "/luckycat/xigua/v1/task/done/ad_watch_daily?",
      "target": "xgsp_sync.js",
      "note": "日常看广告",
      "enable": true,
      "stage": "res"
    },
    {
      "match": "/luckycat/xigua/v:version/task/done/live?",
      "target": "xgsp_sync.js",
      "note": "日常直播",
      "enable": true,
      "stage": "res"
    }
  ],
  "task": {
    "type": "skip",
    "list": [
      {
        "name": "西瓜视频",
        "type": "cron",
        "time": "10 10 11 * * *",
        "job": {
          "type": "runjs",
          "target": "xgsp_sync.js"
        }
      }
    ]
  }
}
